{% extends 'base.html.twig' %}

{% block title %}Clients{% endblock %}

{% block body %}
    <h1>Clients</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ client.id }}</td>
            </tr>
            <tr>
                <th>Roles</th>
                <td>{{ client.roles ? client.roles|json_encode : '' }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ client.email }}</td>
            </tr>
            <tr>
                <th>Username</th>
                <td>{{ client.username }}</td>
            </tr>
            <tr>
                {# <th>Password</th>
                <td>{{ client.password }}</td> #}
            </tr>
            <tr>
                <th>NomClient</th>
                <td>{{ client.nomClient }}</td>
            </tr>
            <tr>
                <th>PrenomClient</th>
                <td>{{ client.prenomClient }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ client.telephone }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_profil_utilisateurs_index') }}">back to list</a>

    {# <a href="{{ path('app_profil_utilisateurs_edit', {'id': client.id}) }}">Modifier</a> #}

    {# {{ include('profil_utilisateurs/_delete_form.html.twig') }} #}
{% endblock %}
